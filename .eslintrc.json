{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["prettier", "airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "no-plusplus": 0,
    "no-continue": 0,
    "no-bitwise": 0
  }
}
